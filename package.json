    "@nordhealth/components": "^4.9.0",
    "@nordhealth/css": "^4.2.0",
    "@nordhealth/fonts": "^3.0.3",
    "@nordhealth/themes": "^9.0.0",
    "@nordhealth/tokens": "^8.0.0",
    "@splidejs/splide": "^4.1.4",
    "google-auth-library": "^10.5.0",
    "google-spreadsheet": "^4.1.5",
    "lodash": "^4.17.21",
